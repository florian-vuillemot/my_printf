#include		"struct_string.h"

int			main(int ac, char **av)
{
  t_string		*str;
  t_string		*str_bis;

  str_bis = str = NULL;
  while ((ac = ac - 1) > 0)
    {
      str = add_string(str, av[ac]);
      str_bis = conca_string(str_bis, str);
    }
  print_string(str);
  printf("\n");
  str = insert_string(str, "coucou", 0);
  print_string(str);
  printf("\n");
  free_string(str);
  free_string(str_bis);
}
